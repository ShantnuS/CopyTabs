{ 
    "manifest_version": 2,
    "name": "CopyTabs", 
    "description": "Copies the open tabs to clipboard", 
    "version": "1.0.0", 
    "author": "Shan",
    "permissions": ["tabs", "clipboardWrite"],
    "browser_action":{
        "default_popup": "popup.html" 	
    },
    "icons": {"30": "copy-content.png"},
    "default_icon": {"30": "copy-content.png"}
}
